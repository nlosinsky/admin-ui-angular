{
  "name": "admin-ui",
  "version": "0.8.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4300",
    "build": "ng build",
    "build:dev": "npm run build -- -c development",
    "build:demo": "npm run build -- -c demo",
    "build:prod": "npm run build -- -c production",
    "test": "ng test",
    "lint:ts": "ng lint",
    "lint:styles": "stylelint **/*.scss",
    "lint": "npm run lint:ts && npm run lint:styles",
    "fix:ts": "eslint --fix \"./src/**/*.ts\"",
    "fix:styles": "stylelint --fix \"**/*.scss\"",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "fix": "npm run prettier && npm run fix:ts && npm run fix:styles",
    "prettier:diff": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --check \"./src\"",
    "prebuild": "rm -rf ./dist",
    "compress:images": "gulp images",
    "gzip": "gulp gzip",
    "postbuild": "npm run compress:images && npm run gzip",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^14.0.0",
    "@angular/animations": "^14.0.3",
    "@angular/common": "^14.0.3",
    "@angular/compiler": "^14.0.3",
    "@angular/core": "^14.0.3",
    "@angular/forms": "^14.0.3",
    "@angular/platform-browser": "^14.0.3",
    "@angular/platform-browser-dynamic": "^14.0.3",
    "@angular/router": "^14.0.3",
    "canvg": "^4.0.0",
    "date-fns": "^2.28.0",
    "devextreme": "^22.1.3",
    "devextreme-angular": "^22.1.3",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "ngx-quicklink": "^0.2.7",
    "ngx-webstorage": "^10.0.1",
    "rxjs": "^7.5.5",
    "sanitize.css": "^13.0.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.3",
    "@angular-eslint/builder": "^14.0.0",
    "@angular-eslint/eslint-plugin": "^14.0.0",
    "@angular-eslint/eslint-plugin-template": "^14.0.0",
    "@angular-eslint/schematics": "^14.0.0",
    "@angular-eslint/template-parser": "^14.0.0",
    "@angular/cli": "^14.0.3",
    "@angular/compiler-cli": "^14.0.3",
    "@types/date-fns": "^2.6.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "^4.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^14.18.21",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.13.0",
    "gulp": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "gulp-imagemin": "^7.1.0",
    "husky": "^8.0.0",
    "imagemin-pngquant": "^9.0.2",
    "jasmine-core": "^4.2.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "~4.7.4"
  }
}
