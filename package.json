{
  "name": "admin-ui-angular",
  "version": "0.9.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4300",
    "build": "ng build -c production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint:ts": "ng lint",
    "lint:styles": "stylelint **/*.scss",
    "lint": "npm run lint:ts && npm run lint:styles",
    "fix:ts": "eslint --fix \"./src/**/*.{ts,js}\"",
    "fix:styles": "stylelint --fix \"**/*.scss\"",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "fix": "npm run prettier && npm run fix:ts && npm run fix:styles",
    "prettier:diff": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "prebuild": "rm -rf ./dist",
    "prepare": "husky install",
    "serve:ssr:admin-ui-angular": "node dist/server/server.mjs",
    "analyze": "ng build --configuration=production --stats-json && npx esbuild-visualizer --metadata=dist/stats.json --open=true"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.6",
    "@angular/compiler": "^20.0.6",
    "@angular/core": "^20.0.6",
    "@angular/forms": "^20.0.6",
    "@angular/platform-browser": "^20.0.6",
    "@angular/platform-server": "^20.0.6",
    "@angular/router": "^20.0.6",
    "@angular/ssr": "^20.0.5",
    "canvg": "^4.0.3",
    "compression": "^1.8.0",
    "date-fns": "^4.1.0",
    "devextreme": "^25.1.3",
    "devextreme-angular": "^25.1.3",
    "exceljs": "^4.4.0",
    "express": "5.1",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "ngx-cookie-service-ssr": "^20.0.1",
    "rxjs": "^7.8.2",
    "sanitize.css": "^13.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^20.0.5",
    "@angular/cli": "^20.0.5",
    "@angular/compiler-cli": "^20.0.6",
    "@eslint/js": "^9.30.1",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "^5.1.8",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.0.10",
    "angular-eslint": "20.1.1",
    "eslint": "^9.30.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-sonarjs": "^3.0.4",
    "husky": "^9.1.7",
    "jasmine-core": "^5.8.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "stylelint": "^16.21.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-prettier": "^5.0.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  }
}
