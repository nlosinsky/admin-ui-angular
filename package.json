{
  "name": "admin-ui-angular",
  "version": "0.9.3",
  "scripts": {
    "ng": "ng",
    "start:front": "ng serve --port=4300",
    "start:server": "npx nodemon server/index.js",
    "start": "npx concurrently \"npm run start:front\" \"npm run start:server\"",
    "build": "ng build -c production",
    "test": "ng test",
    "lint:ts": "ng lint",
    "lint:styles": "stylelint **/*.scss",
    "lint": "npm run lint:ts && npm run lint:styles",
    "fix:ts": "eslint --fix \"./src/**/*.{ts,js}\"",
    "fix:styles": "stylelint --fix \"**/*.scss\"",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "fix": "npm run prettier && npm run fix:ts && npm run fix:styles",
    "prettier:diff": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "prebuild": "rm -rf ./dist",
    "prepare": "husky install",
    "serve:ssr:admin-ui-angular": "node dist/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.4",
    "@angular/common": "^19.2.4",
    "@angular/compiler": "^19.2.4",
    "@angular/core": "^19.2.4",
    "@angular/forms": "^19.2.4",
    "@angular/platform-browser": "^19.2.4",
    "@angular/platform-browser-dynamic": "^19.2.4",
    "@angular/platform-server": "^19.2.4",
    "@angular/router": "^19.2.4",
    "@angular/ssr": "^19.2.5",
    "canvg": "^4.0.3",
    "compression": "^1.8.0",
    "date-fns": "^4.1.0",
    "devextreme": "^24.2.6",
    "devextreme-angular": "^24.2.6",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "ngx-cookie-service-ssr": "^19.1.2",
    "rxjs": "^7.5.5",
    "sanitize.css": "^13.0.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.5",
    "@angular/cli": "^19.2.5",
    "@angular/compiler-cli": "^19.2.4",
    "@eslint/js": "^9.23.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.17",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "^5.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.0",
    "angular-eslint": "19.3.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "eslint": "^9.23.0",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-sonarjs": "^3.0.2",
    "husky": "^9.1.7",
    "jasmine-core": "^5.6.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "stylelint": "^16.17.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.11.1",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0"
  }
