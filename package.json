{
  "name": "admin-ui-angular",
  "version": "0.9.2",
  "scripts": {
    "ng": "ng",
    "start:front": "ng serve --port=4300",
    "start:server": "npx nodemon server/index.js",
    "start": "npx concurrently \"npm run start:front\" \"npm run start:server\"",
    "build": "ng build -c production",
    "test": "ng test",
    "lint:ts": "ng lint",
    "lint:styles": "stylelint **/*.scss",
    "lint": "npm run lint:ts && npm run lint:styles",
    "fix:ts": "eslint --fix \"./src/**/*.ts\"",
    "fix:styles": "stylelint --fix \"**/*.scss\"",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "fix": "npm run prettier && npm run fix:ts && npm run fix:styles",
    "prettier:diff": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --check \"./src\"",
    "prebuild": "rm -rf ./dist",
    "compress:images": "gulp images",
    "gzip": "gulp gzip",
    "postbuild": "npm run compress:images && npm run gzip",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^17.0.2",
    "@angular/animations": "^17.3.12",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "canvg": "^4.0.0",
    "date-fns": "^2.30.0",
    "devextreme": "24.1.11",
    "devextreme-angular": "24.1.11",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "ngx-quicklink": "^0.4.4",
    "ngx-webstorage": "^13.0.1",
    "rxjs": "^7.5.5",
    "sanitize.css": "^13.0.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.13",
    "@angular-eslint/builder": "^17.5.3",
    "@angular-eslint/eslint-plugin": "^17.5.3",
    "@angular-eslint/eslint-plugin-template": "^17.5.3",
    "@angular-eslint/schematics": "^17.5.3",
    "@angular-eslint/template-parser": "^17.5.3",
    "@angular/cli": "^17.3.13",
    "@angular/compiler-cli": "^17.3.12",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "^5.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.81",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "cors": "^2.8.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jasmine": "^4.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-rxjs": "^5.0.3",
    "eslint-plugin-sonarjs": "^0.13.0",
    "express": "^4.21.2",
    "gulp": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "gulp-imagemin": "^7.1.0",
    "husky": "^8.0.0",
    "imagemin-pngquant": "^9.0.2",
    "jasmine-core": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "~5.4.5"
  }
}